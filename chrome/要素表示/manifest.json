{
  "name": "scraping test",
  "description": "scraping test",
  "version": "0.0.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
   "default_popup": "popup.html"
  },

  "content_scripts": [
   {
     "matches": [ "http://*/*", "https://*/*" ],
     "js": [ "jquery-3.5.1.min.js","content.js" ]
   }],

  "permissions": [
    "tabs"
  ]
}
